<head>
  <title>Word Game </title>

</head>
<link rel="stylesheet" type="text/css" href="http://fonts.googleapis.com/css?family=PT Mono|Duru Sans">
<body>
  <div id="wrapper">
    <center><p id="statusz" class="large-font-format">Welcome</p></center>
    <div id="wordstringdiv">
  <span class="wordstring"><span id="first"></span><span id="second"></span><span id="third"></span><span id="last_2"></span><span id="extras"></span></span>




  </div>

   <form id="formz">
    <input type="text" id="inputz" maxlength="15" autocomplete="off" autofocus><br>
  </form>
     <center><p id="hidestring" class="hide" data-hs=""></p></center>
    <center><p id="playstring"></p></center>
    <center><div id="jump-score-container" class="large-font-format">
    <div id="jump-container">Jumps: <span id="jumps" data-j="0">0</span></div>
    <div id="score-container">Score: <span id="score" data-s="0">0</span></div>
  </div></center>
</body>
